select I.SUMMARY, IL.ID as LISTNUMBER
from ISSUES I
join ISSUESLISTS IL ON IL.ID = I.ISSUESLIST_ID;

select I.SUMMARY as ISSUENAME, U.FIRSTNAME, U.LASTNAME
from ISSUES I
join USERS U ON U.ID = I.USER_ID_ASSIGNEDTO;

select U.FIRSTNAME, U.LASTNAME, count(*) as ISSUESSNUMBER
from USERS U
join ISSUES I ON U.ID = I.USER_ID_ASSIGNEDTO
group by I.USER_ID_ASSIGNEDTO
having count(*) > 0
order by U.LASTNAME;